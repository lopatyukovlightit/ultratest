{
  "name": "ultratest-app",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "npm run migrate:run && npm run fixtures:run && nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "build": "rimraf dist && tsc",
    "start:prod": "NODE_ENV=production npm run migrate:run-prod && npm run fixtures:run-prod  && node -r ./dist/tsconfig-paths-bootstrap.js dist/src/main.js",
    "start:hmr": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "webpack": "webpack --config webpack.config.js",
    "ts-node:typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "node:typeorm": "node -r ./dist/tsconfig-paths-bootstrap.js ./node_modules/.bin/typeorm",
    "ts-node:fixtures": "ts-node -r tsconfig-paths/register ./node_modules/.bin/fixtures -c ormconfig.ts --no-color ./fixtures/",
    "node:fixtures": "node -r ./dist/tsconfig-paths-bootstrap.js ./node_modules/.bin/fixtures -c ./dist/ormconfig.js --no-color ./fixtures/",
    "fixtures:run": "npm run --silent ts-node:fixtures",
    "fixtures:run-prod": "npm run --silent node:fixtures",
    "migrate:create": "npm run ts-node:typeorm migration:create -- -n",
    "migrate:generate": "npm run ts-node:typeorm  migration:generate -- -n",
    "migrate:run": "npm run ts-node:typeorm migration:run",
    "migrate:revert": "npm run ts-node:typeorm migration:revert",
    "migrate:run-prod": "NODE_ENV=production npm run node:typeorm migration:run -- -f ./dist/ormconfig.js",
    "migrate:revert-prod": "NODE_ENV=production npm run node:typeorm migration:revert -- -f ./dist/ormconfig.js"
  },
  "dependencies": {
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@nestjs/jwt": "^0.3.0",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/typeorm": "^5.3.0",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "dotenv": "^6.2.0",
    "jsonwebtoken": "^8.5.0",
    "moment": "^2.24.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.8.1",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "rxjs": "^6.2.2",
    "tsconfig-paths": "^3.8.0",
    "typeorm": "^0.2.14",
    "typeorm-fixtures-cli": "^1.1.1",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.7.1",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/passport-jwt": "^3.0.1",
    "@types/request": "^2.48.1",
    "@types/request-promise": "^4.1.42",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tslint": "5.11.0",
    "tslint-plugin-prettier": "^2.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  }
}
